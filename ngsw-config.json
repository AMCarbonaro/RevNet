{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|ico)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-messages",
      "urls": ["/api/revnet/messages/**"],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "1h",
        "strategy": "freshness"
      }
    },
    {
      "name": "api-servers",
      "urls": ["/api/revnet/servers/**"],
      "cacheConfig": {
        "maxSize": 50,
        "maxAge": "30m",
        "strategy": "freshness"
      }
    },
    {
      "name": "api-channels",
      "urls": ["/api/revnet/channels/**"],
      "cacheConfig": {
        "maxSize": 50,
        "maxAge": "30m",
        "strategy": "freshness"
      }
    }
  ]
}
