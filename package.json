{
  "name": "revnet",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npx @angular/cli serve",
    "build": "npx @angular/cli build",
    "build:prod": "npx @angular/cli build --configuration production",
    "serve:prod": "npx serve -s dist/revnet -l 3000",
    "build:ssr": "npx @angular/cli build --configuration production && npx @angular/cli run revnet:build-ssr:production",
    "serve:ssr": "npx @angular/cli run revnet:serve-ssr",
    "watch": "npx @angular/cli build --watch --configuration development",
    "test": "npx @angular/cli test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-server": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@angular/ssr": "^17.3.0",
    "@ngrx/effects": "^17.0.0",
    "@ngrx/entity": "^17.0.0",
    "@ngrx/router-store": "^17.0.0",
    "@ngrx/store": "^17.0.0",
    "@ngrx/store-devtools": "^17.0.0",
    "@stripe/stripe-js": "^2.0.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.0",
    "autoprefixer": "^10.4.0",
    "axios": "^1.6.0",
    "date-fns": "^2.30.0",
    "express": "^5.1.0",
    "gsap": "^3.12.0",
    "lodash": "^4.17.0",
    "lottie-web": "^5.12.0",
    "postcss": "^8.4.0",
    "rxjs": "~7.8.0",
    "serve": "^14.2.0",
    "socket.io-client": "^4.7.0",
    "tailwindcss": "^3.4.0",
    "tslib": "^2.8.1",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.0",
    "@angular/cli": "^17.3.0",
    "@angular/compiler-cli": "^17.3.0",
    "@types/express": "^4.17.21",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.19.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.4.0"
  }
}
